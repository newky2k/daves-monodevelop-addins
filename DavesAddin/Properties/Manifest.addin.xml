<?xml version="1.0" encoding="UTF-8"?>
<Addin namespace="DavesAddin" id="DavesAddin" name="DavesAddin" author="davehumphreys" copyright="davehumphreys" description="DavesAddin" category="IDE extensions" version="1.0">
	<Dependencies>
		<Addin id="::MonoDevelop.Core" version="4.0" />
		<Addin id="::MonoDevelop.Ide" version="4.0" />
		<Addin id="::MonoDevelop.DesignerSupport" version="4.0" />
		<Addin id="::MonoDevelop.SourceEditor2" version="4.0" />
	</Dependencies>
	<Runtime>
		<Import assembly="DavesAddin.dll" />
	</Runtime>
	<Extension path = "/MonoDevelop/Ide/Commands/Edit">  
        <Command id = "DavesAddin.CodeFoldingCommands.CollapseToDefs"  
                 _label = "Collapse to definitions"  
                 _description = "Collapse to definitions"
                 shortcut = "⌃M ⌃O"
                  defaultHandler = "DavesAddin.CodeFoldingHandler" />  
    </Extension>  
    <Extension path = "/MonoDevelop/Ide/MainMenu/Edit/Folding">  
        <CommandItem id="DavesAddin.CodeFoldingCommands.CollapseToDefs" />  
    </Extension>  
</Addin>
